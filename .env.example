# ============================================================
# FAL AI
# Docs: https://docs.fal.ai/reference/client-libraries/javascript
# Base URL: https://queue.fal.run/fal-ai
# Auth: Authorization: Bearer <key>
# Default method: queue + poll status, fetch result on complete
# ============================================================
FAL_API_KEY=your_fal_api_key
FAL_BASE_URL=https://queue.fal.run/fal-ai

# ============================================================
# OPENAI
# Docs: https://developers.openai.com/api/docs/quickstart
# Base URL: https://api.openai.com/v1
# Auth: Authorization: Bearer <key>
# Default method: stream, monitor status, fetch result on complete
# ============================================================
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com/v1

# ============================================================
# WAVESPEED AI
# Docs: https://wavespeed.ai/docs/get-started-api
# Base URL: https://api.wavespeed.ai/api/v3
# Auth: Authorization: Bearer <key>
# Default method: polling
# ============================================================
WAVESPEED_API_KEY=your_wavespeed_api_key
WAVESPEED_BASE_URL=https://api.wavespeed.ai/api/v3

# ============================================================
# SERP API
# Docs: https://serpapi.com/search-api
# Base URL: https://serpapi.com
# Default method: search parameters in URL
# ============================================================
SERPAPI_API_KEY=your_serpapi_api_key
SERPAPI_BASE_URL=https://serpapi.com

# ============================================================
# GOOGLE OAUTH
# Docs: https://docs.n8n.io/integrations/builtin/credentials/google/oauth-single-service/
# ============================================================
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
